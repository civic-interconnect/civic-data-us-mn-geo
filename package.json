{
  "name": "civic-data-boundaries-us-mn",
  "version": "2025.04.4",
  "description": "Minnesota boundary data adapter with validation",
  "type": "module",
  "main": "src/adapter.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "node --test --test-reporter=spec tests/adapter.test.js",
    "test:integration": "node --test --test-reporter=spec tests/adapter.integration.test.js",
    "test:watch": "node --test --watch --test-reporter=spec tests/*.test.js",
    "validate": "npm run validate:catalog",
    "validate:catalog": "node scripts/validate-catalog.js",
    "check": "npm run test && npm run validate",
    "ci": "npm run test:unit && npm run test:integration && npm run validate"
  },
  "dependencies": {},
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.39.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "CC-BY-4.0",
  "keywords": [
    "minnesota",
    "precincts",
    "boundaries",
    "geojson",
    "elections",
    "civic-data",
    "congressional-districts"
  ],
  "author": "Denise Case, Civic Interconnect",
  "repository": {
    "type": "git",
    "url": "https://github.com/denisecase/civic-data-us-mn.git"
  }
}
